<h1 class="heading-xlarge">Users
    <small>Bishop Ullathorne Catholic School</small>
</h1>
<div class="row">
    <div class="col-6 pull-right">
        <div class="pull-right">
            <a href="add-user.html" class="button button-secondary">Invite user</a>
        </div>
    </div>
</div>
<div class="row meta">
    <div class="col-6">
        <p>Showing 1 - 13 of <b>13</b> users</p>
    </div>
    <div class="col-6">

    </div>
</div>
<div class="row">
    <div class="col-12">
        <table class="data">
            <thead>
            <tr class="sortable">
                <th scope="col" class="cwp-15">
                    <a href="#" class="sorted dir-d">Name</a></th>
                <th scope="col" class="cwp-35">
                    <a href="#" class="">Email</a></th>
                <th scope="col" class="cwp-15">
                    <a href="#" class="">Permission level</a></th>
                <th scope="col" class="cwp-10">
                    <a href="#" class="">Status</a></th>
            </tr>
            </thead>
            <tbody>

            <% for(let i= 0; i < locals.usersForOrganisation.length; i++) {%>
            <tr>
                <td><a href="user-profilev1.1.html"><%=locals.usersForOrganisation[i].usersName%></a></td>

                <td><span class="breakable"><%=locals.usersForOrganisation[i].usersEmail%></span></td>
                <td>End user</td>
                <td>Active</td>
            </tr>
            <% } %>
            </tbody>
        </table>
    </div>
</div>

<div class="row meta">
    <div class="col-6">
        <p>Showing 1 - 13 of <b>13</b> users</p>
    </div>
    <div class="col-6">

    </div>
</div>

